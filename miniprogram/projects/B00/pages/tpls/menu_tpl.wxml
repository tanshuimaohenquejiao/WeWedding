<template name="menuTpl"> 
	<view class="tab-bar" >   
		<view wx:if="{{returnHome}}" class="tab-bar-home" bindtap="url" data-type="relaunch"
			data-url="/pages/home/index/home_index">
			<text class="tab-bar-home-text icon-homefill text-grey"></text>
		</view>

		<view class="tab-bar-border"></view>
		<view class="tab-bar-item" bindtap="url" data-type="relaunch" data-url="../../default/index/default_index">
			<image class="tab-icon" src="../../../images/tabbar/home{{curMenu=='home_index'?'_cur':''}}.png"></image>
			<view class="tab-text {{curMenu=='home_index'?'menu-project':''}}">首页</view>
		</view>

		<view class="tab-bar-item" bindtap="url" data-type="relaunch" data-url="../../album/index/album_index">
			<image class="tab-icon" src="../../../images/tabbar/album{{curMenu=='album_index'?'_cur':''}}.png"></image>
			<view class="tab-text {{curMenu=='album_index'?'menu-project':''}}">样片</view>
		</view>

		<view wx:if="{{skin.cate2!==false}}" class="tab-bar-item" bindtap="url" data-type="relaunch" data-url="../../product/index/product_index">
			<image class="tab-icon" src="../../../images/tabbar/product{{curMenu=='product_index'?'_cur':''}}.png"></image>
			<view class="tab-text {{curMenu=='product_index'?'menu-project':''}}">套系</view>
		</view>
		 
		<view class="tab-bar-item" bindtap="url" data-type="relaunch" data-url="../../meet/calendar/meet_calendar">
			<image class="tab-icon" src="../../../images/tabbar/day{{curMenu=='calendar_index'?'_cur':''}}.png"></image>
			<view class="tab-text {{curMenu=='calendar_index'?'menu-project':''}}">预约日历</view>
		</view>
		<view class="tab-bar-item" bindtap="url" data-type="relaunch" data-url="../../my/index/my_index">
			<image class="tab-icon" src="../../../images/tabbar/my{{curMenu=='my_index'?'_cur':''}}.png"></image>
			<view class="tab-text {{curMenu=='my_index'?'menu-project':''}}">我的</view>
		</view>


	</view>
</template>